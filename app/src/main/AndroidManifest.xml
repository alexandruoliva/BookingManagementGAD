<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookingmanagementgad">

    <permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BookingManagementGAD">
        <activity android:name=".GUI.ACTIVITIES.UpdateSingleBookingActivity"></activity>
        <activity android:name=".GUI.ACTIVITIES.EditBookingsActivity" />
        <activity android:name=".GUI.ACTIVITIES.NewBookingActivity" />
        <activity android:name=".GUI.ACTIVITIES.ProfileActivity" />
        <activity android:name=".GUI.ACTIVITIES.DashboardActivity" />
        <activity android:name=".GUI.ACTIVITIES.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GUI.ACTIVITIES.RegisterActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>